package exercicio1;

import java.util.Locale;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);
		
		int hi = sc.nextInt();
		int hf = sc.nextInt();
		
	
		int duracao;
		
		
		if (hi < hf) {
			
			duracao = hf -hi;
		}
		else 
		
		{ duracao = 24 - hi + hf ;}
			System.out.println("o jogo durou: " + duracao);
	
	
	sc.close();
	
	}	}
